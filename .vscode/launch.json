{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run serial_app.exe (MinGW64)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/serial_app.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",

      "environment": [
        {
          "name": "PATH",
          "value": "C:/msys64/mingw64/bin;${env:PATH}"
        }
      ],

      "externalConsole": false,

      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",

      "setupCommands": [
        {
          "description": "Enable pretty-printing",
          "text": "-enable-pretty-printing"
        }
      ]
    }
  ]
}
